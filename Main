#Question 1
def count_vowels(word):
    vowel = 'aeiouAEIOU'
    # Counter for vowels
    count = 0
    for char in word:
        if char in vowel:
            count += 1
    return count

word= "Hello"
print(f"Vowels in '{word}' is: {count_vowels(word)}")

animals = ['tiger','elephant','monkey','zebra','panther']

#Question 2
for animal in animals:
    print(animal.upper())

#Question 3
for number in range(1, 21):
    if number % 2 == 0:
        print(f"{number} is even")
    else:
        print(f"{number} is odd")

#Question 4
def sum_of_integers(a, b):
    return a + b
a=1
b=1
print(f"Sum is: {sum_of_integers(a, b)}")
